{
  "global invariants": [
    "(aes_step == 1) && (aes_state == aes_state_operate) -> byte_counter == 0",
    "(operated_bytes_count_next < aes_reg_oplen) -> operated_bytes_count == byte_counter",
    "uaes_ctr == aes_reg_ctr + block_ctr",
  ],

  "instructions": [
    {
      "instruction" : "LOAD",
      "ready signal": "$xram_ack_monitor.delay_1",
    },
    {
      "instruction"         : "OPERATE",
      "ready signal"        : "( aes_step == 1) && ( aes_reg_state_next == 2'd3 )",
      "child instruction"   : { "trigger bound:" : 1 }
    },
    {
      "instruction" : "STORE",
      "ready bound" : "$xram_ack_monitor.delay_1"
    },
  ]
}

